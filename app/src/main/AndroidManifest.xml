<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.shopingdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".base.IApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/CAppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.IActivity" />
        <activity android:name=".base.BaseMvpActivity" />
        <activity
            android:name=".activitys.TabActivity"
            android:theme="@style/TabAppTheme" />
        <activity android:name=".activitys.PermissionActivity" />
        <activity android:name=".activitys.PermissionActivity1"></activity>
    </application>

</manifest>